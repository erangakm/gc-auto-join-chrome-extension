{
  "name": "Google Calendar video meeting auto join",
  "description": "Automatically join video meetings",
  "version": "1.0",
  "manifest_version": 3,
  "key": "",
  "oauth2": {
    "client_id": "432934095230-ficijfesgpoccdn47v59cl4ul0elk0vl.apps.googleusercontent.com",
    "scopes": [
      "email",
      "https://www.googleapis.com/auth/calendar.events.readonly",
