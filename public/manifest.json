  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "tabs",
    "identity.email"
 ],
  "background": {
    "service_worker": "background.js"
  }
}
