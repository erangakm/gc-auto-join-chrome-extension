{
  "name": "Google Calendar video meeting auto join",
  "description": "Automatically join video meetings",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Schedule your meetings to be auto-joined"
  },
  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  },
  "permissions": [
    "activeTab", "alarms", "tabs"
 ],
  "background": {
    "service_worker": "background.js"
  }
}
