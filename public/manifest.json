{
  "name": "Video meetings assistant",
  "description": "Take me to video meetings on time.",
  "version": "1.4",
  "manifest_version": 3,
  "key": "",
  "action": {
    "default_popup": "index.html",
    "default_title": "Schedule your meetings to be auto-joined"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "identity",
    "alarms",
    "storage",
    "notifications"
 ],
  "background": {
    "service_worker": "background.js"
  }
}
